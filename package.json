    "@effect/platform-node": "^0.103.0",
    "@opentui/core": "^0.1.59",
    "@opentui/react": "^0.1.59",
    "@opentui/solid": "^0.1.59",
    "effect": "^3.19.10",
    "react": "^19.0.0",
    "solid-js": "^1.9.10",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "~24.1.0",
    "@types/react": "^19.0.0",
    "@types/ws": "^8.18.1",
    "recast": "^0.23.11",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
