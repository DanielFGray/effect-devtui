    "start": "bun --watch --conditions=browser src/index.tsx",
    "client": "bun example/client.ts"
  },
  "dependencies": {
    "@effect/experimental": "^0.57.11",
    "@effect/language-service": "^0.62.1",
    "@effect/platform": "^0.93.6",
